{
  "expo": {
    "scheme": "com.hotsource.giftlist",
    "name": "giftlist",
    "slug": "giftlist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hotsource.giftlist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hotsource.giftlist"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "hotsource",
            "project": "giftlist"
          }
        }
      ]
    },
    "plugins": ["sentry-expo"],
    "extra": {
      "eas": {
        "projectId": "4dc35dba-b7a8-467c-9cc4-73725f0988f8"
      }
    }
  }
}
